{
  "name": "ms-messenger",
  "version": "1.0.0",
  "description": "messenger microservice",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "coveralls": "nyc --all npm test && nyc report --reporter=text-lcov | coveralls",
    "documentation": "documentation index.js -o docs/ -f html -g",
    "gendocs": "./generate_docs.sh",
    "jshint": "jshint --exclude ./node_modules ."
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^1.2.0",
    "forecast": "^0.2.1",
    "giphy-api": "^1.1.11",
    "joi": "^7.1.0",
    "mongodb": "^2.1.3",
    "ms-utilities": "^4.0.0",
    "opbeat": "^3.6.6",
    "seneca": "^1.0.0",
    "seneca-rabbitmq-transport": "^0.2.1"
  },
  "devDependencies": {
    "ava": "^0.7.0",
    "coveralls": "^2.11.6",
    "hoek": "^3.0.4",
    "jshint": "^2.9.1",
    "nyc": "^5.0.1",
    "precommit-hook": "^3.0.0",
    "proxyquire": "^1.7.5"
  },
  "config": {
    "nyc": {
      "exclude": [
        "**/*.stub.js",
        "**/database.js",
        "index.js",
        "test",
        "test{,-*}.js"
      ],
      "include": [
        "**/*.js"
      ]
    }
  },
  "pre-commit": [
    "jshint"
  ]
}
